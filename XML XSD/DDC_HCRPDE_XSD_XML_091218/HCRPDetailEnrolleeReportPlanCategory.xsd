					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="claimCountCYNoEp" minOccurs="1" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999999999"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="paidClaimAmtAboveCoinsurance" minOccurs="1" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999999999999.99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>			
			<xsd:element name="includedClaimIdentifier" type="vo:HCRPDetailEnrolleeReportClaimCategory" minOccurs="1" maxOccurs="unbounded"/>
			<xsd:element name="includedSubscriberPeriod" type="vo:HCRPDetailEnrolleeReportSubcriberPeriodCategory" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
