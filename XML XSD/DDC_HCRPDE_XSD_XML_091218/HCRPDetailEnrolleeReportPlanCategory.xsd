<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
	targetNamespace="http://vo.edge.fm.cms.hhs.gov"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:impl="http://vo.edge.fm.cms.hhs.gov" 
	xmlns:vo="http://vo.edge.fm.cms.hhs.gov"
	elementFormDefault="qualified">
	<xsd:include schemaLocation="../../../../../../../../xsd/gov/hhs/cms/fm/edge/vo/hcrpde/HCRPDetailEnrolleeReportClaimCategory.xsd"/>	
	<xsd:include schemaLocation="../../../../../../../../xsd/gov/hhs/cms/fm/edge/vo/hcrpde/HCRPDetailEnrolleeReportSubcriberPeriodCategory.xsd"/>	
	<xsd:element name="planIdentifier" type="impl:HCRPDetailEnrolleeReportPlanCategory"/>
	<xsd:complexType name="HCRPDetailEnrolleeReportPlanCategory">
		<xsd:sequence>
			<xsd:element name="insurancePlanIdentifier" minOccurs="1" maxOccurs="1">
